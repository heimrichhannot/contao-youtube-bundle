{
  "name": "heimrichhannot/contao-youtube-bundle",
  "type": "contao-bundle",
  "description": "Youtube Content Element, with responsive support, preview image, play button and many more.",
  "keywords": [
    "contao",
    "youtube",
    "responsive",
    "video",
    "content"
  ],
  "license": "LGPL-3.0-or-later",
  "require": {
    "php": "^7.1",
    "contao/core-bundle": "^4.4",
    "heimrichhannot/contao-utils-bundle": "^2.60",
    "heimrichhannot-contao-components/alertifyjs": "1.11.2"
  },
  "require-dev": {
    "contao/test-case": "^1.1",
    "contao/manager-plugin": "^2.0",
    "friendsofphp/php-cs-fixer": "^2.2",
    "phpunit/phpunit": ">=6.0 <6.5",
    "php-http/guzzle6-adapter": "^1.1",
    "php-http/message-factory": "^1.0.2",
    "php-coveralls/php-coveralls": "^2.0",
    "symfony/phpunit-bridge": "^3.2"
  },
  "autoload": {
    "psr-4": {
      "HeimrichHannot\\YoutubeBundle\\": "src/"
    },
    "classmap": [
      "src/"
    ],
    "exclude-from-classmap": [
      "src/Resources/contao/config/",
      "src/Resources/contao/dca/",
      "src/Resources/contao/languages/",
      "src/Resources/contao/templates/"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "HeimrichHannot\\YoutubeBundle\\Test\\": "tests/"
    }
  },
  "extra": {
    "contao-manager-plugin": "HeimrichHannot\\YoutubeBundle\\ContaoManager\\Plugin",
    "foxy": true
  },
  "suggest": {
    "heimrichhannot/contao-encore-bundle": "Manage dependencies with Symfony Webpack Encore."
  }
}
